{
  "name": "conjure-typescript",
  "version": "0.0.0",
  "bin": {
    "conjure": "./bin/conjure"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc -p src/",
    "circle-generate-npmrc": "circle-generate-npmrc",
    "clean": "rm -rf dist distributions tmp coverage",
    "lint": "tslint 'src/**/*.ts*' --format codeFrame --project ./src/tsconfig.json",
    "lint-fix": "yarn lint -- --fix --project ./src/tsconfig.json",
    "test": "jest --config ../../config/jest.config.js --rootDir ./",
    "verify": "npm-run-all lint test",
    "publish-dist": "npm-run-all create-dist run-publish",
    "run-publish": "../../scripts/publish-dist.sh dist/conjure-typescript.tgz",
    "create-dist": "webpack --config webpack.config.js && tar -czf dist/conjure-typescript.tgz dist/bundle"
  },
  "dependencies": {
    "conjure-client": "^0.2.0",
    "commander": "^2.15.1",
    "download": "^6.2.1",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "semver": "^5.5.0",
    "ts-simple-ast": "^11.2.0"
  },
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/download": "^6.2.1",
    "@types/fs-extra": "^5.0.1",
    "@types/lodash": "^4.14.108",
    "@types/mkdirp": "^0.5.2",
    "@types/nock": "^9.1.2",
    "@types/node-dir": "^0.0.30",
    "@types/semver": "^5.5.0",
    "@types/tempy": "^0.1.0",
    "awesome-typescript-loader": "^5.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "nock": "^9.1.2",
    "node-dir": "^0.1.16",
    "npm-run-all": "^4.1.1",
    "tempy": "^0.2.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.3"
  },
  "author": "Palantir Technologies, Inc.",
  "license": "Apache-2.0"
}
